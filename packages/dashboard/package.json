{
  "name": "@innolens/dashboard",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "homepage": "https://i.cs.hku.hk/fyp/2019/fyp19027/",
  "repository": "https://github.com/StarvinSon/innolens/tree/develop/packages/dashboard",
  "scripts": {
    "clean": "shx rm -rf ./out ./tools ./*.tsbuildinfo",
    "lint": "run-s lint:*",
    "lint:es": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore --ext .js,.ts .",
    "lint:wc": "lit-analyzer ./src",
    "build": "run-s build:tools generate:glue build:dashboard:prod",
    "build:watch": "run-p build:tools:watch generate:glue:watch build:dashboard:dev:watch",
    "build:tools": "tsc --build ./tsconfig-tools.json",
    "build:tools:watch": "tsc --build ./tsconfig-tools.json --watch",
    "generate:glue": "node ./tools/glue-generator.js",
    "generate:glue:watch": "nodemon --watch ../api/spec --watch ./tools --ext js,yaml ./tools/glue-generator.js",
    "build:dashboard:prod": "webpack --config ./webpack.config.js --env production --progress",
    "build:dashboard:dev": "webpack --config ./webpack.config.js --env development --progress",
    "build:dashboard:dev:watch": "webpack --config ./webpack.config.js --env development --progress --watch"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.9.2",
    "@innolens/api": "*",
    "@innolens/resolver": "*",
    "@lit-element-bootstrap/carousel": "^1.0.0-alpha.2",
    "@lit-element-bootstrap/progress": "^1.0.0-alpha.2",
    "core-js": "^3.6.5",
    "d3-array": "^2.4.0",
    "d3-hierarchy": "^1.1.9",
    "d3-scale": "^3.2.1",
    "d3-scale-chromatic": "^1.5.0",
    "d3-shape": "^1.3.7",
    "date-fns": "^2.12.0",
    "date-fns-tz": "^1.0.10",
    "file-loader": "^6.0.0",
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@innolens/eslint-config": "*",
    "@types/babel__core": "^7.1.7",
    "@types/d3-array": "^2.0.0",
    "@types/d3-hierarchy": "^1.1.6",
    "@types/d3-scale": "^2.2.0",
    "@types/d3-scale-chromatic": "^1.5.0",
    "@types/d3-shape": "^1.3.2",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/loader-utils": "^1.1.3",
    "@types/node": "^13.11.1",
    "@types/webpack": "^4.41.11",
    "babel-loader": "^8.1.0",
    "babel-plugin-template-html-minifier": "^4.1.0",
    "css-loader": "^3.5.2",
    "dart-sass": "^1.25.0",
    "eslint": "^6.8.0",
    "fibers": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "lit-analyzer": "^1.1.10",
    "loader-utils": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "reflect-metadata": "^0.1.13",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "shx": "^0.3.2",
    "ts-loader": "^6.2.2",
    "ts-morph": "^7.0.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": ">=13.5.0"
  },
  "private": true
}
